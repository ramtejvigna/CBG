{
  "name": "cbg-code-execution",
  "version": "1.0.0",
  "description": "CBG Code Execution Service - Docker-based code execution",
  "type": "module",
  "scripts": {
    "start": "node build/server.js",
    "dev": "nodemon --exec \"npx tsx\" src/server.ts",
    "build": "tsc",
    "docker:build-sandbox": "docker build -t code-execution-sandbox:latest -f Dockerfile.sandbox .",
    "docker:test": "docker run --rm code-execution-sandbox:latest echo 'Sandbox working!'",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.9",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
