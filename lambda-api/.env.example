# Environment
NODE_ENV=production
AWS_REGION=us-east-1

# Database - PostgreSQL on EC2
DATABASE_URL="postgresql://cbg_user:your_strong_password@your-ec2-private-ip:5432/cbg_production"

# Code Execution Service URL (EC2 private IP)
CODE_EXECUTION_URL="http://your-ec2-private-ip:3002"

# Frontend URL (Vercel)
FRONTEND_URL="https://your-app.vercel.app"

# Authentication
JWT_SECRET="your-super-secret-jwt-key-min-32-chars"
SESSION_SECRET="your-session-secret-key"
